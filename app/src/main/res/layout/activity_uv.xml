<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UV">

  <Button
      android:id="@+id/backtoHazardButton"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:layout_marginEnd="16dp"
      android:layout_marginBottom="16dp"
      android:background="@drawable/btn_grey"
      android:onClick="hazards"
      android:padding="10dp"
      android:text="Back To\n Hazards Page"
      android:textColor="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/navbar7"
      app:layout_constraintVertical_bias="1.0" />

  <ToggleButton
      android:id="@+id/toggleButton7"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="8dp"
      android:layout_marginBottom="16dp"
      android:background="@drawable/toggle_location"
      android:scaleType="fitXY"
      android:textOff=""
      android:textOn=""
      app:layout_constraintBottom_toTopOf="@+id/bannertext2"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.5"
      app:layout_constraintStart_toEndOf="@+id/logo8"
      app:layout_constraintTop_toBottomOf="@+id/settingsicon7"
      app:layout_constraintVertical_bias="0.5" />

  <ImageView
      android:id="@+id/logo8"
      android:layout_width="211dp"
      android:layout_height="86dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/navbar7"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/sgsafetyedit" />

  <ImageView
      android:id="@+id/navbar7"
      android:layout_width="match_parent"
      android:layout_height="68dp"
      android:background="@drawable/red_bar"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/red_bar" />

  <ImageButton
      android:id="@+id/settingsicon7"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="16dp"
      android:background="@drawable/red_bar"
      android:onClick="settings"
      android:scaleType="fitXY"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="@+id/navbar7"
      app:layout_constraintHorizontal_bias="0.95"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/settingsicon" />

  <ImageButton
      android:id="@+id/homeButton7"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="8dp"
      android:background="@drawable/red_bar"
      android:onClick="main"
      android:scaleType="fitXY"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.05"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/homepageicon" />

  <ImageButton
      android:id="@+id/contactsButton7"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:background="@drawable/red_bar"
      android:onClick="contacts"
      android:scaleType="fitXY"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.35"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/contacts_icon" />

  <ImageButton
      android:id="@+id/hazardsButton7"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:layout_marginTop="8dp"
      android:layout_marginEnd="8dp"
      android:background="@drawable/red_bar"
      android:scaleType="fitXY"
      android:onClick="hazards"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="@+id/navbar7"
      app:layout_constraintHorizontal_bias="0.65"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/hazards_icon_white" />

  <TextView
      android:id="@+id/bannertext2"
      android:layout_width="match_parent"
      android:layout_height="24dp"
      android:layout_marginTop="23dp"
      android:background="#FEFD98"
      android:text="Ultra Violet Radiation in Singapore"
      android:textAlignment="center"
      android:textColor="@color/black"
      android:textStyle="normal"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/logo8"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/UVtext"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:text="Current UV\n Radiation\n Level\n Classification"
      android:textAlignment="center"
      android:textColor="@color/black"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.15"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <ImageView
      android:id="@+id/UVimage"
      android:layout_width="100dp"
      android:layout_height="100dp"
      android:layout_marginEnd="16dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.85"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/green_or_red_shield" />


</androidx.constraintlayout.widget.ConstraintLayout>